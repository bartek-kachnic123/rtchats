{
  "name": "rtchats",
  "workspaces": [
    "frontend/*"
  ],
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "frontend:dev": "npm run dev -w frontend/app",
    "frontend:build": "npm run build -w frontend/app",
    "frontend:ci": "npm run ci:build -w frontend/app",
    "frontend-coverage": "npm run coverage -w frontend/app",
    "hooks:setup": "lefthook install",
    "dt": "devtools-cli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bartek-kachnic123/rtchats.git"
  },
  "bugs": {
    "url": "https://github.com/bartek-kachnic123/rtchats/issues"
  },
  "homepage": "https://github.com/bartek-kachnic123/rtchats#readme",
  "description": "",
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@rtchats/devtools": "workspace:*",
    "lefthook": "1.11.14"
  }
}
